{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Person",
  "type": "object",
  "properties": {
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "birthday": {
      "type": "string",
      "format": "date"
    },
    "nationality": {
      "type": "string"
    }
  },
  "required": ["firstname", "lastname", "birthday", "nationality"]
}
